<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault continueOnError="false" enabled="true" name="RF-MissingSecretKey">
  <DisplayName>RF-MissingSecretKey</DisplayName>
  <FaultResponse>
    <Set>
      <Headers/>
      <Payload contentType="application/json"> 
      "error" : "Missing Secret key for JWT generation"
      </Payload>
      <StatusCode>400</StatusCode>
      <ReasonPhrase>Bad request</ReasonPhrase>
    </Set>
  </FaultResponse>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>
